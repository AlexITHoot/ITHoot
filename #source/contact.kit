<!DOCTYPE html>
<html lang="en">

<head>
	<title>Home</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<!-- <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
		integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous"> -->
	<link rel="stylesheet" href="https://unpkg.com/swiper@8/swiper-bundle.min.css" />
	</link>
	<link rel="stylesheet" href="css/style.css">
</head>

<body>
	<div class="main-wrapper">
		<!-- @import "main-kit/header.kit" -->
		<div class="content-wrapper">
			<section class="contact">
				<div class="container">
					<div class="parts-wrapper">
						<div class="contact-part">
							<h5>Request Estimate</h5>
							<form action="post">
								<div class="group-wrapper">
									<div class="group control required">
										<label for="Name">Name</label>
										<input id="Name" name="Name" type="mail" placeholder="">
									</div>
									<div class="group control required">
										<label for="PhoneOrEmail">Phone or Email</label>
										<input id="PhoneOrEmail" name="PhoneOrEmail" type="mail" placeholder="">
									</div>
								</div>
								<div class="group textarea required">
									<label for="Details">Project Details</label>
									<textarea name="Details" id="Details"></textarea>
								</div>
								<div class="part-wrapper">
									<div class="side-part">
										<div class="group file-transparent required">
											<div class="output-group">
												<label for="files">
													<img src="/img/contact/Tilt.svg" alt="">
													Attach a file (20 MB max.)
												</label>
												<input type="file" name="files" id="files" accept="image/*">
												<!-- <p class="file-output"></p> -->
											</div>
										</div>
										<div class="group check">
											<input class="checkbox" type="checkbox" id="accept" name="accept">
											<label class="form-label" for="accept">I accept the terms of privacy and
												processing of my personal data.</label>
											<span class="invalid-feedback"></span>
										</div>
									</div>
									<div class="side-part">
										<button class="btn services">Send Request</button>
									</div>
								</div>
							</form>
						</div>
						<div class="contact-part">
							<div class="contact-info">
								<h5>Contact Info</h5>
								<div class="link-wrapper">
									<a class="link-group" href="#">
										<img src="/img/contact/Phone.svg" alt="">
										<span>+38 (096)-080-85-45</span>
									</a>
									<a class="link-group" href="#">
										<img src="/img/contact/Gmail.svg" alt="">
										<span>info@ithoot.com</span>
									</a>
									<a class="link-group" href="#">
										<img src="/img/contact/Skype.svg" alt="">
										<span>Hoot-skype</span>
									</a>
								</div>
								<div class="link-wrapper">
									<a href="#"><img src="/img/contact/Telegram.svg" alt=""></a>
									<a href="#"><img src="/img/contact/Viber.svg" alt=""></a>
								</div>
							</div>
							<div class="follow-info">
								<h5>Follow Us</h5>
								<div class="link-wrapper">
									<a href="#"><img src="/img/contact/Trased.svg" alt=""></a>
									<a href="#"><img src="/img/contact/UpSocial.svg" alt=""></a>
									<a href="#"><img src="/img/contact/Facebook.svg" alt=""></a>
									<a href="#"><img src="/img/contact/Linkedin.svg" alt=""></a>
									<a href="#"><img src="/img/contact/Twitter.svg" alt=""></a>
								</div>
							</div>
						</div>
					</div>
				</div>
			</section>
		</div>
		<!-- @import "main-kit/footer.kit" -->
	</div>
	<!-- @import "main-kit/js.kit" -->
</body>

</html>