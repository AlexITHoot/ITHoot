<!DOCTYPE html>
<html lang="en">

<head>
	<title>Home</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
		integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
	<link rel="stylesheet" href="https://unpkg.com/swiper@8/swiper-bundle.min.css" />
	</link>
	<link rel="stylesheet" href="css/style.css">
</head>

<body>
	<div class="main-wrapper">
		<!-- @import "main-kit/header.kit" -->
		<div class="content-wrapper">
			<section class="contact">
				<div class="container">
					<div class="contact-part">
						<h4>Request Estimate</h4>
						<form action="">
							<div class="group-wrapper">
								<div class="group control required">
									<label for="Name">Name</label>
									<input id="Name" name="Name" type="mail" placeholder="">
								</div>
								<div class="group control required">
									<label for="PhoneOrEmail">Phone or Email</label>
									<input id="PhoneOrEmail" name="PhoneOrEmail" type="mail" placeholder="">
								</div>
							</div>
							<div class="group textarea required">
								<label for="Details">Project Details</label>
								<textarea name="Details" id="Details"></textarea>
							</div>
							<div class="group file-transparent required">
								<div class="output-group">
									<label for="files">
										<img src="/img/contact/Tilt.svg" alt="">
										Attach a file (20 MB max.)
									</label>
									<input type="file" name="files" id="files" accept="image/*">
									<!-- <p class="file-output"></p> -->
								</div>
							</div>
							<div class="group check">
								<label for="PhoneOrEmail">I accept the terms of privacy and
									processing of my personal data.</label>
								<input id="PhoneOrEmail" name="PhoneOrEmail" type="mail" placeholder="">
							</div>
						</form>
					</div>
					<div class="contact-part">
						<h4>Contact Info</h4>

					</div>
				</div>
			</section>
		</div>
		<!-- @import "main-kit/footer.kit" -->
	</div>
	<!-- @import "main-kit/js.kit" -->
</body>

</html>